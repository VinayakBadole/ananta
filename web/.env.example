# Ananta Website Environment Variables

# Development Configuration
NODE_ENV=development
VITE_APP_NAME=Ananta
VITE_APP_VERSION=1.0.0

# API Configuration (if needed)
# VITE_API_BASE_URL=https://api.example.com
# VITE_API_KEY=your_api_key_here

# Analytics Configuration (if needed)
# VITE_GA_TRACKING_ID=GA-XXXXXXXXX
# VITE_GTM_ID=GTM-XXXXXXX

# Map Configuration (for Leaflet maps)
# VITE_MAP_API_KEY=your_map_api_key
# VITE_MAP_CENTER_LAT=28.6139
# VITE_MAP_CENTER_LNG=77.2090

# Theme Configuration
VITE_DEFAULT_THEME=dawn
VITE_AUTO_THEME=false

# Performance Configuration
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Contact Form Configuration (if using external service)
# VITE_CONTACT_FORM_ENDPOINT=https://formspree.io/f/your-form-id
# VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key

# Social Media Links
# VITE_FACEBOOK_URL=https://facebook.com/ananta
# VITE_TWITTER_URL=https://twitter.com/ananta
# VITE_INSTAGRAM_URL=https://instagram.com/ananta
# VITE_LINKEDIN_URL=https://linkedin.com/company/ananta